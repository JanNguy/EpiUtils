{
    "name": "Epitech Intra UI Overhaul",
    "description": "UI Overhaul for Epitech's Intra",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "js": [ 
                "scripts/config.js",
                "scripts/overhaul.js" 
            ],
            "matches": [
                "https://intra.epitech.eu/",
                "https://intra.epitech.eu/*"
            ]
        }
    ]
}
